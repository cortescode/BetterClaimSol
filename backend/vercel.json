{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "public/static/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "public/assets/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "public/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "public/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "public/index.html"
      }
    ]
  }