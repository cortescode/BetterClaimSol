{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "public/$1"
    },
    {
      "src": "/privacy-policy",
      "dest": "public/provacy-policy.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}