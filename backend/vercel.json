{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "dist/assets/$1"
    },
    {
      "src": "^/(?!api/?$).*\\.(ico|png|jpg|jpeg|svg|css|js|gif|woff|woff2|ttf|eot)$",
      "dest": "dist/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}