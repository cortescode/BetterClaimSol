{
    "version": 2,
    "functions": {
      "api/index.js": {
        "runtime": "@vercel/node"
      }
    },
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
}